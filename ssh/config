Host *
	ControlPath ~/.ssh/sessions/S.%r@%h:%p.sock
	ControlMaster auto
	ControlPersist 1h
	Compression yes
	IdentityFile ~/.ssh/id_rsa
	# Only OSx
	UseKeychain yes
	AddKeysToAgent yes

Host networkradius.com
	IdentityFile ~/.ssh/jpereira@networkradius.com-id_rsa.pub

Host freeradius.org
	IdentityFile ~/.ssh/jpereira@networkradius.com-id_rsa.pub

Host router
	Hostname router.lan
	User root

Host tapioca
	Hostname tapioca.mooo.com
	Port 6922
	User root

Include ~/.ssh/config.hosts

