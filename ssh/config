#
#Host freeradius.org
#	IdentityFile ~/.ssh/jpereira@networkradius.com-id_rsa.pub

Host jp
	Hostname jorgepereira.com.br
	User root
	ForwardAgent yes

Host router
	Hostname router.lan
	User root
	ForwardAgent yes

Include ~/.ssh/config.hosts

