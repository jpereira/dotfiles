Host *
	ControlPath ~/.ssh/sessions/S.%r@%h:%p.sock
	ControlMaster auto
	ControlPersist 1h
	ServerAliveInterval 100
	TCPKeepAlive yes
	Compression yes
	IdentityFile ~/.ssh/id_rsa
	# Only OSx
	UseKeychain yes
	AddKeysToAgent yes

